{
  "name": "react-flexgrid",
  "version": "0.4.2",
  "description": "Flexbox grid for react apps",
  "keywords": [
    "react",
    "grid",
    "flexbox"
  ],
  "author": "Nikita Gusakov <dev@nkt.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/nkt/react-flexgrid.git"
  },
  "homepage": "https://github.com/nkt/react-flexgrid",
  "bugs": {
    "url": "https://github.com/nkt/react-flexgrid/issues"
  },
  "main": "lib/index.js",
  "dependencies": {
    "react": "^0.12.0",
    "babel-runtime": "^5.1.11"
  },
  "devDependencies": {
    "babel": "^5.1.11",
    "babel-eslint": "^3.0.1",
    "babel-jest": "^5.0.1",
    "eslint": "^0.19.0",
    "eslint-plugin-react": "^2.2.0",
    "jest-cli": "^0.4.0"
  },
  "scripts": {
    "prepublish": "babel -d lib/ src/",
    "test": "eslint src && jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "jsx",
      "js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/babel",
      "<rootDir>/node_modules/babel-runtime"
    ]
  }
}
